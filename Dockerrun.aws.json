{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "app",
            "image": "kocuna/app",
            "hostname": "app",
            "essential": false,
            "memory": 300
        },
        {
            "name": "api",
            "image": "kocuna/api",
            "hostname": "api",
            "essential": false,
            "memory": 300
        },
        {
            "name": "nginx",
            "image": "kocuna/nginx",
            "hostname": "nginx",
            "essential": true,
            "memory": 300,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "api",
                "app"
            ]
        }
    ]
}
